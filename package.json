{
  "name": "blog",
  "version": "1.0.0",
  "description": "Simplest CMS to ever exist",
  "exports": "./src/app.js",
  "type": "module",
  "scripts": {
    "start": "node --loader esm-module-alias/loader src/app.js",
    "start:dev": "nodemon --loader esm-module-alias/loader src/app.js",
    "lint": "eslint .",
    "lint-and-fix": "eslint . --fix",
    "test": "NODE_NO_WARNINGS=1 NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "test:watch": "NODE_NO_WARNINGS=1 NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --watch",
    "test:coverage": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --coverage --coverageDirectory=coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codebless-ing/blog.git"
  },
  "author": "Chrystian & Matheus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codebless-ing/blog/issues"
  },
  "homepage": "https://github.com/codebless-ing/blog#readme",
  "dependencies": {
    "dotenv": "^16.4.5",
    "esm-module-alias": "^2.2.0",
    "express": "^4.19.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.1",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "eslint": "^9.7.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0"
  },
  "aliases": {
    "@routes": "src/routes",
    "@controllers": "src/http",
    "@services": "src/app",
    "@models": "src/app/models",
    "@common": "src/common",
    "@middlewares": "src/common/middlewares"
  }
}
